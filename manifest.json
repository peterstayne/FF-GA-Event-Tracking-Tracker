{
  "name": "Event Tracking Tracker",
  "version": "1.1.6",
  "manifest_version": 2,
  "description": "Plugin to track Google Analytics Event Tracker event calls",
  "browser_action": {
    "default_icon": "ett.png"
  },
  "icons": {
    "16": "ett16.png",
    "48": "ett48.png",
    "128": "ett.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "storage",
    "webRequest",
    "tabs", 
    "http://*/*", 
    "https://*/*"
  ]
}